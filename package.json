{
  "private": true,
  "scripts": {
    "dev": "concurrently \"contentlayer dev\" \"next dev --turbo\"",
    "build": "contentlayer build && pnpm prisma:generate && next build",
    "postinstall": "pnpm prisma:generate",
    "contentlayer": "contentlayer build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"{app,lib,components}/**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"{app,lib,components}**/*.{ts,tsx,mdx}\" --cache",
    "prepare": "husky install",
    "test": "vitest --pool=forks --run --testTimeout=1200000",
    "test:watch": "vitest --pool=forks --testTimeout=1200000",
    "inngest": "npx inngest-cli@latest dev",
    "clean": "rm -rf .next node_modules",
    "cli": "DOTENV_CONFIG_PATH=.env.local tsx -r dotenv/config --no-deprecation cli/main.ts",
    "knip": "knip",
    "knip:fix": "knip --fix",
    "swagger": "next-swagger-doc-cli openapi.json --output public/openapi.json",
    "snyk": "snyk code test --org=8944b057-2a22-4806-813e-10dc8c82b5c5",
    "forge:build": "cd contracts && forge build",
    "forge:test": "cd contracts && forge test -vvv",
    "typechain": "rm -rf ./lib/abi && typechain --target ethers-v6 --out-dir ./lib/abi ./contracts/out/**/*.json",
    "prisma:init": "dotenv -e .env.local -- npx prisma migrate dev --name init",
    "prisma:generate": "npx prisma generate --schema ./lib/db/schema.prisma",
    "prisma:migrate:local": "dotenv -e .env.local -- npx prisma migrate dev",
    "prisma:generate:local": "dotenv -e .env.local -- pnpm prisma:generate"
  },
  "prisma": {
    "schema": "lib/db/schema.prisma"
  },
